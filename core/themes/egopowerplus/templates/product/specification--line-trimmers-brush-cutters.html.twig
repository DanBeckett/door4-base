                    <div class="specification">
                    
                        <div class="label">Speed Modes</div>
                        
                        <div class="value">{{ content.field_spec_speed_modes }}</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_no_load_speed_rpm|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    
                        <div class="label">No Load Speed</div>
                        
                        <div class="value">{{ content.field_spec_no_load_speed_rpm }}rpm</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_soft_start|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    
                        <div class="label">Soft Start</div>
                        
                        <div class="value">{{ content.field_spec_soft_start }}</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_line_diameter_mm|render|trim is not empty %}
        

                            <div class="label">{{ 'Line Diameter'|trans }}</div>
                            
                            <div class="value">{{ content.field_spec_line_diameter_mm }}mm</div>
                        
                        </div>
                    
                    </li>

                {% endif %}

                {% if content.field_spec_line_feed|render|trim is not empty %}
            
                    <li>
                    
                        <div class="specification">
                        
                            <div class="label">{{ 'Line Feed'|trans }}</div>
                            
                            <div class="value">{{ content.field_spec_line_feed }}</div>
                        
                        </div>
                    
                    </li>

                {% endif %}

                {% if content.field_spec_dual_line|render|trim is not empty %}
            
                    <li>
                    
                        <div class="specification">
                        
                            <div class="label">{{ 'Dual Line'|trans }}</div>
                            
                            <div class="value">{{ content.field_spec_dual_line }}</div>
                        

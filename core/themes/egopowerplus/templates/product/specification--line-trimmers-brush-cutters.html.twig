                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_dual_line|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    
                        <div class="label">Dual Line</div>
                        
                        <div class="value">{{ content.field_spec_dual_line }}</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_rotating_direction|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    
                        <div class="label">Rotating Direction</div>
                        
                        <div class="value">{{ content.field_spec_rotating_direction }}</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_aux_handle|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    

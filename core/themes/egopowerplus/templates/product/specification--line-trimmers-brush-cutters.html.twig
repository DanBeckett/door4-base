                {% endif %}

                {% if content.field_spec_weight_wo_battery_lth|render|trim is not empty %}
            
                    <li>
                    
                        <div class="specification">
                        
                            <div class="label">{{ 'Weight without Battery, Line Trimmer Head'|trans }}</div>
                            
                            <div class="value">{{ content.field_spec_weight_wo_battery_lth }}kg</div>
                        

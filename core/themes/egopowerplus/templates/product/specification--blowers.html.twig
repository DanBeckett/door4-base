                    <div class="specification">
                    
                        <div class="label">Run Time 5.0Ah - Low</div>
                        
                        <div class="value">{{ content.field_spec_run_time_5_low }}mins</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_run_time_75_boost|render|trim is not empty %}
        

                    <div class="specification">
                    
                        <div class="label">Run Time 4.0Ah - High</div>
                        
                        <div class="value">{{ content.field_spec_run_time_4_high }}mins</div>
                    
                    </div>
                
                </li>

            {% endif %}

            {% if content.field_spec_run_time_4_low|render|trim is not empty %}
        
                <li>
                
                    <div class="specification">
                    
                        <div class="label">Run Time 4.0Ah - Low</div>
                        
                        <div class="value">{{ content.field_spec_run_time_4_low }}mins</div>
                    
                    </div>
                
                </li>

            {% endif %}


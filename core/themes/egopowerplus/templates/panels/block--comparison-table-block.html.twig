              </tr>
              <tr class="weight">
                <td colspan="2" class="label">Weight</td>
                <td>1.3kg</td>
                <td>1.3kg</td>
                <td>1.3kg</td>
                <td>1.3kg</td>
                <td>1.3kg</td>
                <td>7.8kg without harness</td>
              </tr>
            </tbody>
          </table>

          {% if content.body %}
            <div class="footnote">{{ content.body }}</div>
          {% endif %}
      
      </div><!-- .batteries_table -->

    {% elseif content.field_table_to_display['#items'][0].value == 'standard' %}

      <div class="standard_table">
    
        {% if content.field_title %}
          <h2>{{ content.field_title }}</h2>
        {% endif %}
          
          <table class="standard" width="" border="0" cellspacing="0" cellpadding="0">
            <tbody>
              <tr>
                <th scope="col"></th>
                <th scope="col">CS1400E</th>
                <th scope="col">CS1600E</th>
              </tr>
              <tr>
                <th scope="row">Blade length</th>
                <td>35cm</td>
                <td>35cm</td>
              </tr>
              <tr>
                <th scope="row">Chain speed</th>
                <td>12.0m/s</td>
                <td>12.0m/s</td>
              </tr>
              <tr>
                <th scope="row">Run time*</th>
                <td>&nbsp;</td>

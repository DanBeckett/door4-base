              </tr>
              <tr>
                <th scope="row">Chain type</th>
                <td>AC1400</td>
                <td>AC1400</td>
              </tr>
              <tr>
                <th scope="row">Oil reservoir</th>
                <td>150ml</td>
                <td>150ml</td>
              </tr>
              <tr>
                <th scope="row">Weight without battery</th>
                <td>3.7kg</td>
                <td>3.7kg</td>
              </tr>
            </tbody>
          </table>
                  
          {% if content.body %}
            <div class="footnote">{{ content.body }}</div>
          {% endif %}

      </div>

    {% endif %}
  {% endblock %}
</div>

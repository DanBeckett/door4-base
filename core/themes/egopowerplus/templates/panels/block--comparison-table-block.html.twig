          <tbody>
            <tr class="headers">
              <th scope="row">&nbsp;</th>
              <td colspan="3" scope="col">LB6000E</td>
              <td colspan="3" scope="col">LB4800E</td>
              <td colspan="3" scope="col">LB5300E</td>
              <td colspan="3" scope="col">LB5750E</td>
              </tr>
            <tr class="shaded">
              <th scope="row">Constant Speed</th>
              <td colspan="3">Yes</td>
              <td colspan="3">Yes</td>
              <td colspan="3">Yes</td>
              <td colspan="3">Yes</td>
            </tr>
            <tr class="reduce-padding-bottom">
              <th scope="row">Run Time</th>
              <td class="no-right-border"><strong>2.5Ah</strong></td>
              <td class="no-right-border"><strong>5.0Ah</strong></td>
              <td><strong>7.5Ah</strong></td>
              <td class="no-right-border"><strong>2.0Ah</strong></td>
              <td class="no-right-border"><strong>5.0Ah</strong></td>
              <td><strong>7.5Ah</strong></td>
              <td class="no-right-border"><strong>2.5Ah</strong></td>
              <td class="no-right-border"><strong>5.0Ah</strong></td>
              <td><strong>7.5Ah</strong></td>
              <td class="no-right-border"><strong>2.5Ah</strong></td>
              <td class="no-right-border"><strong>5.0Ah</strong></td>
              <td><strong>7.5Ah</strong></td>
            </tr>
            <tr class="reduce-padding-top reduce-padding-bottom">
              <th scope="row">Low Speed</th>
              <td class="no-right-border">-</td>
              <td class="no-right-border">Up to 120 mins</td>
              <td>-</td>
              <td class="no-right-border">Up to 60 mins</td>
              <td class="no-right-border">Up to 150 mins</td>
              <td>Up to 225 mins</td>
              <td class="no-right-border">Up to 75 mins</td>
              <td class="no-right-border">Up to 150 mins</td>
              <td>Up to 225 mins</td>
              <td class="no-right-border">-</td>
              <td class="no-right-border">Up to 150 mins</td>
              <td>Up to 225 mins</td>
            </tr>
            <tr class="reduce-padding-top reduce-padding-bottom">
              <th scope="row">High Speed</th>
              <td class="no-right-border">-</td>
              <td class="no-right-border">Up to 22 mins</td>
              <td>-</td>
              <td class="no-right-border">Up to 18 mins</td>
              <td class="no-right-border">Up to 45 mins</td>
              <td>Up to 70 mins</td>
              <td class="no-right-border">Up to 22 mins</td>
              <td class="no-right-border">Up to 44 mins</td>
              <td>Up to 66 mins</td>
              <td class="no-right-border">-</td>
              <td class="no-right-border">Up to 18 mins</td>
              <td>Up to 25 mins</td>
            </tr>
            <tr class="reduce-padding-top">
              <th scope="row">Boost</th>
              <td class="no-right-border">-</td>
              <td class="no-right-border">Up to 15 mins</td>
              <td>-</td>
              <td class="no-right-border">Up to 10 mins</td>
              <td class="no-right-border">Up to 25 mins</td>
              <td>Up to 35 mins</td>
              <td class="no-right-border">Up to 10 mins</td>
              <td class="no-right-border">Up to 20 mins</td>
              <td>Up to 30 mins</td>
              <td class="no-right-border">-</td>
              <td class="no-right-border">-</td>
              <td>-</td>
            </tr>
            <tr class="shaded reduce-padding-bottom">
              <th scope="row">&nbsp;</th>
              <td class="no-right-border"><strong>LOW</strong></td>
              <td class="no-right-border"><strong>HIGH</strong></td>
              <td><strong>BOOST</strong></td>
              <td class="no-right-border"><strong>LOW</strong></td>
              <td class="no-right-border"><strong>HIGH</strong></td>
              <td><strong>BOOST</strong></td>
              <td class="no-right-border"><strong>LOW</strong></td>

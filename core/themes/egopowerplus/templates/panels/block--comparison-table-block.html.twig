              <td colspan="3">Brushless</td>
            </tr>
            <tr class="shaded">
              <th scope="row">Noise Rating</th>
              <td colspan="3">83LpA / 97LwA</td>
              <td colspan="3">82LpA / 97LwA</td>
              <td colspan="3">82LpA / 97LwA</td>
              <td colspan="3">83LpA / 95LwA</td>
            </tr>
            <tr>
              <th scope="row">Vibration (m/s<sup>2</sup>)</th>
              <td colspan="3">1.5</td>
              <td colspan="3">1.1</td>
              <td colspan="3">1.35</td>
              <td colspan="3">1.4</td>
            </tr>
          </tbody>
        </table>

        {% if content.body %}
          <div class="footnote">{{ content.body }}</div>
        {% endif %}
    
      </div>

    {% elseif content.field_table_to_display['#items'][0].value == 'batteries' %}

      <div class="batteries_table">
      
        {% if content.field_title %}
          <div class="title">{{ content.field_title }}</div>
        {% endif %}

          <table class="batteries" width="100%" border="0" cellspacing="0" cellpadding="0">
            <tbody>
              <tr class="title">
                <td colspan="8">Performance statistics</td>
              </tr>
              <tr class="battery">
                <td colspan="2" class="label">Battery</td>
                <td>2.0Ah</td>
                <td>2.0Ah</td>
                <td>2.0Ah</td>
                <td>2.0Ah</td>
                <td>2.0Ah</td>
                <td>Backpack Battery</td>
              </tr>
              <tr class="capacity">
                <td colspan="2" class="label">Capacity (WH/AH)</td>
                <td>112/2.0</td>
                <td>112/2.0</td>
                <td>112/2.0</td>
                <td>112/2.0</td>
                <td>112/2.0</td>
                <td>1299Wh at 56v rated</td>
              </tr>
              <tr class="charge_times">
                <td rowspan="2" class="label">Charge times</td>
                <td class="label">Rapid charger</td>
                <td>25 mins</td>
                <td>25 mins</td>
                <td>25 mins</td>
                <td>25 mins</td>
                <td>25 mins</td>
                <td>XX mins</td>
              </tr>
